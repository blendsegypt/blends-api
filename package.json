{
  "name": "blends-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node -r esm ./bin/www",
    "mkmigrate": "node -r esm ./node_modules/sequelize-auto-migrations/bin/makemigration.js",
    "dbmigrate": "sequelize-cli db:migrate",
    "updatemigrate": "rm -f ./migrations/_current.json && rm -f ./migrations/_current_bak.json && node -r esm ./node_modules/sequelize-auto-migrations/bin/makemigration.js && cd ./migrations && rm -f **-noname.js"
  },
  "dependencies": {
    "aws-sdk": "^2.794.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "express": "~4.16.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "node-cron": "^2.0.3",
    "node-fetch": "^2.6.1",
    "pg": "^8.4.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.3.5",
    "sequelize-auto-migrations": "github:blendsegypt/sequelize-auto-migrations"
  },
  "devDependencies": {
    "nodemon": "^2.0.6",
    "sequelize-cli": "^6.2.0"
  }
}
